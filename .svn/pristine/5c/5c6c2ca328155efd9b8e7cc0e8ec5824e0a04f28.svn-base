                }
            }
        }
    }
}
/* 工具栏 */

.shop-toolbar {
    position: fixed;
    right: 0;
    bottom: 45px;
    left: 0;
    height: 44px;
    border-top: 1px solid #ddd;
    background-color: #fff !important;
    line-height: 44px;
    z-index: 100;
    &.bottom{
        bottom: 0 !important;
    }
    input[type="checkbox"] {
        top: 10px;
        left: 12px !important;
    }
    label {
        // padding-left: 40px !important;
        em {
            font-size: 12px;
            font-style: normal;
        }
        span.total {
            margin-left: 20px;
            font-size: 15px !important;
        }
        .tip-text {
            color: #f35432;
        }
    }
    button {
        position: absolute;
        top: 0;
        right: 0;
        bottom: 0;
        width: 110px !important;
        height: 100%;
        border: none;
        border-radius: 0;
        background-color: @base;
        color: #fff;
    }
    .collect-btn {
        right: 110px;
        background-color: @base;
    }
    .clear-btn {
        background-color: @red;
    }
}

/* 弹出层 */

.dialogMask,
.shoppingMask {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 999;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.3);
}

.dialog-wrapper {
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 1000;
    overflow: hidden;
    width: 100%;
    height: auto;
    border-radius: 10px 10px 0 0;
    background-color: #fff;
    .dialog-box {
        position: relative;
        padding-bottom: 50px;
