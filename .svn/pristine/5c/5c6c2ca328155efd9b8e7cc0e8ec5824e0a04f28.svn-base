/* 覆盖mint-ui的primary颜色，改为自己UI的主题色 */

@base: #eb6000;
@red: #eb6000;
.fixed {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    width: 100%;
    z-index: 10;
}

body,
dd,
dl,
dt,
fieldset,
form,
h1,
h2,
h3,
h4,
h5,
h6,
html,
img,
legend,
li,
ol,
p,
ul {
    margin: 0;
    padding: 0;
    -moz-user-select: none;
    -webkit-user-select: none;
    -webkit-tap-highlight-color: transparent;
}

button,
fieldset,
img,
input {
    border: none;
    padding: 0;
    margin: 0;
    outline-style: none;
}

button[disabled] {
    background-color: #888888 !important;
}

ol,
ul {
    list-style: none;
}

input {
    padding-top: 0;
    padding-bottom: 0;
    font-family: pingFangSC-Regular;
}

input,
select {
    vertical-align: middle;
}
input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {
    -webkit-appearance: none;
    appearance: none; 
    margin: 0; 
}
/* 火狐 */

input[type="number"]{-moz-appearance:textfield}
input,
select,
textarea {
    font-size: 14px;
    margin: 0;
    height: auto;
    border: none;
}

textarea {
    resize: none;
}

/*防止拖动*/

img {
    border: 0;
    vertical-align: middle;
}

/*  去掉图片低测默认的3像素空白缝隙*/

table {
    border-collapse: collapse;
}

//去斜体
i {
    font-style: normal;
}

a {
    color: #666;
    text-decoration: none;
}

h1,
h2,
h3,
h4,
h5,
h6 {
    text-decoration: none;
    font-weight: normal;
    font-size: 100%;
}

.ac {
    text-align: center;
}

.clearfix {
    zoom: 1;
}

.clearfix:after,
.clearfix:before {
    display: table;
    content: "";
}

.clearfix:after {
    clear: both;
    display: block;
    visibility: hidden;
    height: 0;
    content: ".";
}

ul,
ol {
    list-style: none;
}

a {
    text-decoration: none;
    outline: none;
}

input::-webkit-input-placeholder,
textarea::-webkit-input-placeholder {
    color: #b3b3b3;
    font-size: 14px;
    font-family: PpingFangSC-Regular !important;
}

input::-moz-placeholder,
textarea::-moz-placeholder {
    color: #b3b3b3;
    font-size: 14px;
    font-family: PpingFangSC-Regular !important;
}

input:-ms-input-placeholder,
textarea:-ms-input-placeholder {
    color: #b3b3b3;
    font-size: 14px;
    font-family: PpingFangSC-Regular !important;
}

.my-content {
    padding-top: 45px;
}

.left {
    float: left !important;
}

.right {
    float: right !important;
}

.mt-10 {
    margin-top: 10px;
}

.dec-text {
    font-size: 12px;
    color: #888888;
}
.iconfont.def{
    color: @base;
}
.bottom-btn{
    position: fixed;
    width: 100%;
    left:0;
    bottom:0;
    button{
        border-radius: 0;
    }
    // padding: 15px;
}
.def-btn{
    margin-top: 30px;
    appearance: none;
    border-radius: 4px;
    border: 0;
    box-sizing: border-box;
    color: #fff;
    display: block;
    font-size: 18px;
    width: 100%;
    height: 41px;
    outline: 0;
    overflow: hidden;
    position: relative;
    text-align: center;
    background-color: @base;
}
.header {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    z-index: 99;
}

.form-wrapper {
    .select-address {
        display: inline-flex;
        width: 100%;
        select {
            flex: 1;
        }
    }
    input.mint-field-core{
        border: none;
    }
    .mint-field-other{
        position: absolute;
        top:18px;
        left:4px;
        width: 6px;
        color:red;
    }
}
.mint-cell-label,.mint-cell-title{
    font-size: 14px !important;
    color: #333333 !important;
    margin-top: 0 !important;
}
.my-cell .mint-field-core {
    text-align: right;
    font-size: 14px;
    color: #333333;
}
.mint-cell-value span {
    font-size: 14px;
    color: #b3b3b3;
    &.active{
        color: #333333;
    }
}
.mint-cell span.tag{
    color: @base;
}
.my-cell .mint-cell-value span{
    font-size: 14px;
    color: #333333;
}
.mint-msgbox-confirm{
    color: @base !important;
}
.my-cell .mint-cell-value .mint-field-core{
    color: #333333;
    font-size: 14px;
}
.cell-link{
    .mint-cell-title{
        flex: 0 0 120px;
    }
}
.msg-con{
    padding: 10px;
}
/* mui全局样式 */
.mui-checkbox input[type=checkbox]:checked:before, .mui-radio input[type=radio]:checked:before{
    color: @base;
}
.mui-checkbox input[type="checkbox"]:before,
.mui-radio input[type="radio"]:before {
    font-size: 22px;
}
.mui-badge-danger.mui-badge-inverted,
.mui-badge-red.mui-badge-inverted {
    color: @red  !important;
    background-color: transparent;
}
input[type=color], input[type=date], input[type=datetime-local], input[type=datetime], input[type=email], input[type=month], input[type=number], input[type=password], input[type=search], input[type=tel], input[type=text], input[type=time], input[type=url], input[type=week], select, textarea{
    // border: none;
    margin: 0;
}

.select-radio {
    position: relative;
    height: 28px;
    line-height: 28px;
    font-size: 14px;
    display: inline-block;
    input{
        position: absolute;
        top:4px !important;
        left:15px !important;
        &::before{
            font-size: 18px
        }
    }
     label {
        padding-right: 0 !important;
        padding-left: 38px !important;
        font-size: 12px !important;
        color: #666666;
        width: 88px !important;
        text-align: left;
    }
}


/* 商品列表全局样式 */

.goods-list {
    overflow: hidden;
    overflow-y: scroll;
    height: auto;
    // background-color: #fff;
    li {
        position: relative;
        display: block;
        box-sizing: border-box;
        padding: 9pt;
        width: 100%;
        height: auto;
        background-color: #fff;
        input[type="checkbox"] {
            top: 25px;
            left: 0 !important;
        }
        .car-info {
            width: 100%;
            padding-left: 30px;
            .mint-cell{
                .mint-cell-value{
                    width: 100%;
                }
            }
        }
        .slot_box {
            width: 100%;
        }
        &::after {
            position: absolute;
            right: 0;
            bottom: 0;
            left: 15px;
            height: 1px;
            content: "";
            -webkit-transform: scaleY(0.5);
            transform: scaleY(0.5);
            background-color: #dddddd;
        }
        .favory-box {
            position: absolute;
            right: 10px;
            bottom: 10px;
            i.iconfont {
                color: #ff671d;
                font-size: 24px;
            }
        }
        .gift-link {
            position: relative;
            height: 100%;
            padding: 10px 0; 
            margin-top: 10px;
            // border-top:1px solid #eee;
            .span_tip{
                color: @base;
                font-size: 12px;
            }
            .is_gift {
                position: absolute;
                top: 10px;
                left: 0;
                width: 50px;
                height: 18px;
                background-color: rgba(235, 96, 0, 0.7);
                color: #fff;
                text-align: center;
                font-size: 9pt;
                line-height: 18px;
            }
            img {
                float: left;
                width: 55px;
                height: 55px;
            }
            .gift-info{
                margin-left: 60px;
                text-align: left;
                position: relative;
                overflow: hidden;
               
                .skuName {
                    margin-bottom: 5px;
                    color: #333333;
                    font-size: 14px;
                    width: 100%;
                    line-height: 1.4;
                    font-weight: 500;
                    overflow: hidden;
                    text-overflow: ellipsis;
                    white-space: nowrap;
                    &.shotName{
                        width: 72% !important;
                    }
                }
                p {
                    width: 100%;
                    font-size: 12px;
                    line-height:17px;
                    color: #999;
                    font-size: 12px;
                }
                .gift-num{
                    position: absolute;
                    top:20px;
                    right:0;
                    color: #888888;
                }
            }
        }
        .food-link {
            position: relative;
            height: 100%;
            img {
                float: left;
                width: 55px;
                height: 55px;
                // margin:0 10px 0 6px;
            }
            .gift-img{
				position: absolute;
				top:10px;
				left:10px;
				width: 20px;
				height: 20px;
				border: 1px solid @base;
				border-radius:3px;
                background-color: #fff;
                img{
                    margin: 0;
                    float: none;
                    width:100%;
                    height: 100%;
                }
				span{
					position: absolute;
					color: @base;
				}
			}
            .is_enable {
                position: absolute;
                top: 0;
                left: 0;
                width: 50px;
                height: 18px;
                background-color: rgba(0, 0, 0, 0.6);
                color: #fff;
                text-align: center;
                font-size: 9pt;
                line-height: 18px;
            }
            div.recommend {
                margin-left: 60px;
                text-align: left;
                position: relative;
                min-height: 70px;
                overflow: hidden;
                .skuName {
                    margin-bottom: 5px;
                    color: #333333;
                    font-size: 14px;
                    width: 100%;
                    line-height: 1.4;
                    font-weight: 500;
                    overflow: hidden;
                    text-overflow: ellipsis;
                    white-space: nowrap;
                    &.shotName{
                        width: 72% !important;
                    }
                }
                p {
                    width: 100%;
                    font-size: 12px;
                    line-height:17px;
                    margin-bottom: 4px;
                    color: #999;
                    font-size: 12px;
                }
                .tag {
                    display: block;
                    color: @base;
                    width: 30px;
                    text-align: center;
                    height: 18px;
                    line-height: 14px;
                    font-size: 10px;
                    border: 1px solid @base;
                    border-radius: 4px;
                    padding: 2px 2px;
                }
                .kuc{
                    position: absolute;
                    top: 0;
                    right: 0; 
                    text-align:right;
                    font-size:11px;
                    line-height: 1.2;
                    color: @red;
                }
                .price {
                    display: block;
                    font-size: 14px;
                    line-height: 1.4;
                    color: #f35432;
                    // margin-top: 10px;
                    &.hide-price{
                        font-size: 12px;
                        color: @red;
                    }
                    em{
                        color: #888888;
                        font-size: 12px;
                        margin-left: 4px;
                        text-decoration: line-through;
                    }

                    .retail_price {
                        color: #888888;
                        font-size: 10px;
                    }
                    .begin{
                        color: @red;
                        font-size: 12px;
                        margin-left: 4px;
                    }
                }
                .tip{
                    font-size: 10px;
                    margin-top: 4px;
                    color: #999999;
                }
                .store {
                    position: absolute;
                    bottom: 0;
                    right: 0;
                    font-size: 12px;
                    color: #999999;
                    height: 18px;
                }
            }
        }
        i.gift {
            margin-right: 4px;
            font-size: 20px;
            color: #ff671d !important;
            vertical-align: middle;
        }
        .cartcontrol-wrapper {
            position: absolute;
            right: 15px;
            bottom: 12px;
            .countcontrol {
                .unit-count,
                .uom-count {
                    padding: 0 4px;
                    width: 45px;
                    height: 28px;
                    line-height: 16px;
                    text-align: center;
                    font-size: 14px;
                    color: #333333;
                    border-radius: 3px;
                    border: 1px solid #ddd;
                    margin-bottom: 0;
                }
            }
            .unit,
            .uom_default {
                width: auto;
                height: 100%;
                font-size: 12px;
                list-style: 24px;
                vertical-align: middle;
                color: #888;
            }
        }
    }
}

.mescroll-empty {
    position: fixed;
    top: 50%;
    left: 50%;
    width: 100%;
    text-align: center;
    transform: translate3d(-50%, -50%, 0);
}

/* 购物车控制 */

.cartcontrol {
    font-size: 0;
    border: 1px solid #ddd;
    height: 32px;
    // border-radius: 5px;
    overflow: hidden;
    .cart-count {
        display: inline-block;
        vertical-align: top;
        padding: 0 4px;
        width: 45px;
        height: 100%;
        line-height: 16px;
        text-align: center;
        font-size: 14px;
        color: #666666;
        border-radius: 0;
        border-top: none;
        border-left: 1px solid #ddd;
        border-right: 1px solid #ddd;
        border-bottom: none;
        margin-bottom: 0;
    }
    .cart-add,
    .cart-decrease {
        display: inline-block;
        vertical-align: top;
        width: 30px;
        height: 100%;
        font-size: 24px;
        font-weight: 400;
        line-height: 100%;
        font-weight: 500;
        border-radius: 0;
        color: #eb6000;
        background-color: #fff;
    }
}


/* 登录注册 */

